  <servlet>
    <servlet-name>UploadFormServlet</servlet-name>
    <servlet-class>com.icss.servlet.UploadFormServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RegServlet</servlet-name>
    <servlet-class>com.icss.servlet.RegServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>ResetPwdServlet</servlet-name>
    <servlet-class>com.icss.servlet.ResetPwdServlet</servlet-class>
