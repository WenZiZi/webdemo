  <servlet>
    <servlet-name>DownloadServlet</servlet-name>
    <servlet-class>com.icss.servlet.DownloadServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>findTypeFilesServlet</servlet-name>
